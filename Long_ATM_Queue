#include <iostream>
using namespace std;

int main()
{
    int n,h[1000000];
    cin>>n;
    
    for(int i=0;i<n;i++)
    {
        cin>>h[i];
    }
    int c=0;
    for(int i=0;i<n;i++)
    {
        if(h[i]>h[i+1])
            c++;
    }
    cout<<c;
    //cout << "Hello World!" << endl;
    return 0;
}
