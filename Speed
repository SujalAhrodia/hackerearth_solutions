#include <bits/stdc++.h>
using namespace std;

int main()
{
    //cout << "Hello World!" << endl;
    int t;
    cin>>t;
    
    while(t--)
    {
        int n,c=0,p=INT_MAX,a;
        cin>>n;
        
        for(int i=0;i<n;i++)
        {
            cin>>a;
            if(p>a)
            {  
                c++;
                p=a;
            }
        }
        cout<<c<<endl;
    }
    return 0;
}
